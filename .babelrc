{
  "presets": [["env", {
    "modules": false,
    "loose": false,
    "useBuiltIns": false,
    "targets": {
      "ie": 10,
      "uglify": true
    }
  }], "react"],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true
    }],
    ["transform-regenerator", {
      "async": false
    }],
    ["module-resolver", {
      "root": ["./"],
      "alias": {}
    }]
  ],
  "env": {
    "test": {
      "presets": ["env", "react"],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
        ["transform-runtime", {
          "helpers": false,
          "polyfill": false,
          "regenerator": true
        }],
        ["transform-regenerator", {
          "async": false
        }],
        ["module-resolver", {
          "root": ["./"],
          "alias": {}
        }]
      ]
    }
  }
}
